#!/usr/bin/env ruby

ENV['RAILS_ENV'] = 'development'

require_relative '../config/environment'

proxy = UpvsEnvironment.upvs_proxy(assertion: nil)
service = EformService.new(proxy)

# TC_EFORM_01

service.fetch_xsd_schema('App.GeneralAgenda', '1.9')

# TC_EFORM_01_NEG

begin
  service.fetch_xsd_schema('App.UnknownAgenda', '1.0')
rescue javax.xml.ws.soap.SOAPFaultException
  # ignore
end

# TC_EFORM_02

service.fetch_all_form_template_ids
